{
  "name": "serverless-request-handler",
  "description": "A request handler that uses class-transformer and class-validator to create a type-safe serverless handler!",
  "version": "1.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenvanrooij/serverless-request-handler.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist",
    "build": "yarn clean && tsc",
    "format": "prettier --write {src,test}/**/*.ts",
    "lint": "tslint --project . src/**/*.ts",
    "prepare": "install-self-peers -- --ignore-scripts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "author": "Ruben v. Rooij",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@team-griffin/install-self-peers": "^1.1.1",
    "@types/aws-lambda": "^8.10.36",
    "@types/jest": "^24.0.23",
    "@types/prettier": "^1.19.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/rimraf": "^2.0.3",
    "codecov": "^3.6.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "semantic-release": "^15.13.31",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "aws-lambda": "^1.0.5",
    "class-validator-jsonschema": "^1.3.0",
    "http-status-codes": "^1.4.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.2.1"
  },
  "peerDependencies": {
    "aws-xray-sdk": "^3.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1"
  }
}
